



var andrey = new Person("Andrey");
andrey.age = 30;

andrey.setAge(32).setName('Gaurav').setLocation('India');


class Person {
  name: string = ''';
  age: number;

  construct(name: string) {
    this.name = name;
  }

  setAge = (age: number) => {
    this.age = age;
    return this;
  }

  setName = (name: string) => {
    this.name = name;
    return this;
  }


  .setLocation('India')
}

export Person;